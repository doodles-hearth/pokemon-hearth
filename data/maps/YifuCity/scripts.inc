	hidemoneybox
	bufferspeciesname STR_VAR_1, SPECIES_CATERPIE
	compare VAR_RESULT, MON_GIVEN_TO_PARTY
	goto_if_eq YifuCity_ShinySellerChoice_13
	compare VAR_RESULT, MON_GIVEN_TO_PC
