script Common_EventScript_SetGymTrainers {
	switch(var(VAR_0x8008)) {
		case 1: call(KoiPond_Dojo_SetGymTrainers)
		case 2: call(Bonsai_Dojo_SetGymTrainers)
		case 3: call(YifuCity_Dojo_SetGymTrainers)
		case 4: call(LavaridgeTown_Gym_SetGymTrainers)
		case 5: call(PetalburgCity_Gym_SetGymTrainers)
		case 6: call(FortreeCity_Gym_SetGymTrainers)
		case 7: call(MossdeepCity_Gym_SetGymTrainers)
		case 8: call(SootopolisCity_Gym_SetGymTrainers)
	}
}

script KoiPond_Dojo_SetGymTrainers {
	settrainerflag(TRAINER_IZUMI)
	settrainerflag(TRAINER_REN)
	settrainerflag(TRAINER_MIZU)
	return
}

